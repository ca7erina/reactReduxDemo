{
    "extends": "airbnb",
    "plugins": [
        "import",
        "react"
      ],
    "root": true,
    "env": {
        "browser": true,
        "jest":true
    },
    "rules": {
        "jsx-a11y/anchor-is-valid": [ "error", {
            "components": [ "Link" ],
            "specialLink": [ "to" ]
          }]
    }
}